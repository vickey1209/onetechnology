      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
    />

    <style>
  ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
}

li {
  
  float: left;
}
</style>
</head>
<body>

    </style>
  </head>
  <style type="text/css">
    table,
    td,
    th {
      border: 1px solid rgb(17, 13, 13);
      text-align: left;
      font-size:small;
    }

    table {
      border-collapse: collapse;
      width: 75%;
    }
    .table-data {
      position: relative;
      left: 50px;
      top: 50px;
    }
    th,
    td {
      padding: 15px;
    }
    a {
      padding-right: 15px;
    }


    body {
  background:linear-gradient(to right, #8f6a92 0%, #a4a5a2 50%, #52a7c9 99%);
}

  
  </style>

  <body>
      <h1 style="text-align: center; color: rgb(255, 0, 157);" > <u> ALL REGISTERED USERS</u> </h1>
     

    
    

      <br>


    <% if(details!=null) { %>
    <table style="text-align: center; color: rgb(24, 7, 173); font-weight: 500px; table-layout: auto; text-decoration: solid;">
      
      <tr class="hint">
        <th>Sr.N</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
     
       

        </h1>
      </tr>
      <% details.forEach(function(item,index){ %>
      <tr>
        <td><%= index+1%></td> 
        <td><%= item.firstname%></td>
        <td><%= item.lastname%></td>
        <td><%= item.email%></td>

        </tr>
      
      <% }) %>
    </table>
    <% } %>
    

    
   

    
  </body>
 
</html>

